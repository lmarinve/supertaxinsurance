<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="insurance_detail_sequence" model="ir.sequence">
        <field name="name">Insurance Detail</field>
        <field name="code">insurance.detail</field>
        <field name="prefix">INS/</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="claim_detail_sequence" model="ir.sequence">
        <field name="name">Claim Detail</field>
        <field name="code">claim.detail</field>
        <field name="prefix">CLM/</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
