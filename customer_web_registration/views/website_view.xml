<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template active="True" id="website.option_header_brand_logo" inherit_id="website.placeholder_header_brand" name="Header Brand Logo">
            <xpath expr="//*[@id='o_fake_navbar_brand']" position="replace">
                <a href="/" t-attf-class="navbar-brand logo #{_link_class}">
                    <span role="img" t-att-aria-label="'Logo of %s' % website.name" t-att-title="website.name" t-field="website.logo" t-options="{'widget': 'image'}"/>
                </a>
                <t t-if="not request.session.uid">
                    <a class="nav-link fw-bold" role="button" t-attf-href="/web/signup/individual?{{ keep_query() }}">
                        Sign Up
                    </a>
                </t>
            </xpath>
        </template>
    </data>
</odoo>
