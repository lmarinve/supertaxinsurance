<?xml version="1.0" encoding="utf-8"?>
<!-- inherit contact us page and modify contact us form and when submit redirect to contact us thankyou page-->
<odoo>
	<template id="contactus_custom" inherit_id="website_crm.contactus_form">
		<xpath expr="//div[@id='wrap']" position="replace">
			<div class="oe_structure">
				<section class="s_title pt32 undefined pb0" data-name="Title" data-resize-width="1920" data-snippet="s_title" data-vcss="001">
					<div class="o_we_shape o_web_editor_Blobs_12"/>
					<div class="s_allow_columns container">
						<h1>
							<font class="text-center" style="font-size: 35px;">
								<b>Contact Us</b>
							</font>
						</h1>
						<h1>
							<font class="text-o-color-3" style="font-size: 48px;">
								<b>
									<br/>
								</b>
							</font>
						</h1>
					</div>
				</section>
				<section class="s_picture bg-200 o_colored_level pt0 pb0" data-name="Picture" data-snippet="s_picture" style="">
					<div class="container-fluid">
						<div class="row s_nb_column_fixed">
							<div class="col-lg-12 o_colored_level pb0" style="text-align: center;">
								<figure class="figure"/>
								<figure class="figure"/>
								<br/>
							</div>
						</div>
					</div>
				</section>
				<section class="s_text_block pt32 o_colored_level pb0" data-name="Text" data-snippet="s_text_block">
					<div class="s_allow_columns container">
						<p>
							Great stories have a
							<b class="o_default_snippet_text">
								personality
							</b>
							. Consider telling a great story that provides personality. Writing a story with personality for potential clients will assist with making a relationship connection. This shows up in small quirks like word choices or phrases. Write from your point of view, not from someone else's experience.
						</p>
					</div>
				</section>
				<div class="row">
					<div class="col-2"/>
					<div class="col-4" style="padding-right:100px; border-right: 1px solid #ccc;">
						<section class="s_website_form pt24 pb18 o_colored_level undefined" data-name="contact us form" data-snippet="s_website_form" data-vcss="001" style="">
							<div class="o_container_small">
								<form action="/submitted/contact/lead" class="form-horizontal mt32" enctype="multipart/form-data" method="POST">
									<input name="csrf_token" t-att-value="request.csrf_token()" type="hidden"/>
									<div class="s_website_form_rows row s_col_no_bgcolor">
										<div class="container">
											<div class="row">
												<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char" style="visibility:hidden;">
													<label class="s_website_form_label" for="check_counter_id" style="width: 100px"/>
													<input class="form-control s_website_form_input" id="check_counter_id" name="check_counter_id" type="checkbox"/>
												</div>
											</div>
											<div class="row">
												<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char" id="khyyke60o4c">
													<label class="s_website_form_label" for="khyyke60o4c" style="width: 200px">
														<b>
															<span>
																Company Name
															</span>
														</b>
													</label>
													<input class="form-control s_website_form_input" name="company_name" type="text" id="khyyke60o4c" required="1"/>
												</div>
											</div>
											<div class="row">
												<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char" id="khyyke60o4c">
													<label class="s_website_form_label" for="khyyke60o4c" style="width: 200px">
														<b>
															<span>
																Company Address
															</span>
														</b>
													</label>
													<input class="form-control s_website_form_input" id="khyyke60o4c" name="company_address" type="text" required="1"/>
												</div>
											</div>
											<div class="row">
												<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char" id="khyyke60o4c">
													<label class="s_website_form_label" for="khyyke60o4c" style="width: 200px">
														<b>
															<span>
																No of Employees
															</span>
														</b>
													</label>
													<select class="form-control s_website_form_input" id="khyyke60o4c" name="no_of_employees" required="1">
														<option value=""/>
														<option value=">20">
															>20
														</option>
														<option value="20-50">
															20-50
														</option>
														<option value="50-70">
															50-70
														</option>
														<option value="70-100">
															70-100
														</option>
													</select>
												</div>
											</div>
											<div class="row">
												<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char" id="khyyke60o4c">
													<label class="s_website_form_label" for="khyyke60o4c" style="width: 200px">
														<b>
															<span>
																Industry Sector
															</span>
														</b>
													</label>
													<input class="form-control s_website_form_input" id="khyyke60o4c" name="industry_sector" type="text" required="1"/>
												</div>
											</div>
											<div class="row">
												<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="many2one" id="khyyke60o4c">
													<label class="s_website_form_label" for="khyyke60o4c" style="width: 200px">
														<b>
															<span>
																Country
															</span>
														</b>
													</label>
													<select class="form-control" name="country_id" id="country_id_onchange" required="1">
														<option value=""/>
														<t t-as="country_id" t-foreach="request.env['res.country'].search([]) or []">
															<option t-att-value="country_id.id" t-esc="country_id.name"/>
														</t>
													</select>
												</div>
											</div>
											<div class="row">
												<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char" id="khyyke60o4c">
													<label class="s_website_form_label" for="khyyke60o4c" style="width: 200px">
														<b>
															<span>
																Select Course
															</span>
														</b>
													</label>
													<select class="form-control" name="course_id" id="course_id_onchange" required="1">
														<option value=""/>
														<t t-as="course_id" t-foreach="request.env['slide.channel'].search([]) or []">
															<option t-att-value="course_id.id" t-esc="course_id.name"/>
														</t>
													</select>
												</div>
											</div>
											<div class="row">
												<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char" id="khyyke60o4c">
													<label class="s_website_form_label" for="khyyke60o4c" style="width: 300px">
														<b>
															<span>
																No of Employees to be registered
															</span>
														</b>
													</label>
													<input class="form-control s_website_form_input" id="khyyke60o4c" name="no_registered_employee" required="1" type="number"/>
												</div>
											</div>
											<div class="row">
												<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char" id="khyyke60o4c">
													<label class="s_website_form_label" style="width: 200px" for="khyyke60o4c">
														<b>
															<span>
																Contract Term
															</span>
														</b>
													</label>
													<select class="form-control s_website_form_input" id="khyyke60o4c" name="contract_term" required="1">
														<option value=""/>
														<option value="6 months">
															6 Months
														</option>
														<option value="1 year">
															1 Year
														</option>
														<option value="2 years">
															2 Years
														</option>
														<option value="&gt;2 years">
															&amp;gt;2 Years
														</option>
													</select>
												</div>
											</div>
											<div class="row">
												<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char" id="khyyke60o4c">
													<label class="s_website_form_label" style="width: 200px" for="khyyke60o4c">
														<b>
															<span>
																Contact Details
															</span>
														</b>
													</label>
													<input class="form-control s_website_form_input" id="khyyke60o4c" name="name" pattern="[a-zA-Z ]*" placeholder="Name" required="1" title="Please Enter Alphabets Only" type="text"/>
													<br/>
													<input class="form-control s_website_form_input" id="khyyke60o4c" name="designation" placeholder="Designation" required="true" type="text"/>
													<br/>
													<input class="form-control s_website_form_input" id="khyyke60o4c" name="phone" placeholder="Phone" required="true" type="text"/>
													<br/>
													<input class="form-control s_website_form_input" id="khyyke60o4c" name="email_from" placeholder="Email" required="true" type="email"/>
												</div>
											</div>
											<div class="row">
												<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required o_colored_level col-lg-12" data-name="Field" data-type="text" id="khyyke60o4c">
													<label class="s_website_form_label " for="khyyke60o4c" style="width: 200px">
														<b>
															<span>
																Comments/Notes
															</span>
														</b>
													</label>
													<textarea class="form-control s_website_form_input" id="khyyke60o4c" name="description" placeholder="Comments/Notes" required="1" rows="1" style="height: 100px;"/>
												</div>
											</div>
											<br/>
											<br/>
											<div class="row">
												<div class="form-group col-12 s_website_form_submit col-lg-12 pb12 pt12 text-center">
													<div style="width: 100px !important;"/>
													<button class="btn btn-primary btn-lg saturate_send" style="width: 140px !important;">
														SUBMIT
													</button>
													<span class="text-danger ml4"/>
												</div>
											</div>
										</div>
									</div>
								</form>
							</div>
						</section>
					</div>
					<div class="col-6">
						<div class="container">
							<div class="row">
								<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char"/>
							</div>
							<div class="row">
								<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char"/>
							</div>
							<div class="row">
								<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char"/>
							</div>
							<div class="row">
								<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char"/>
							</div>
							<div class="row">
								<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char"/>
							</div>
							<div class="row">
								<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char">
									<img class="saturate" style="max-height: 100%;max-width:9%;object-fit: cover;" t-att-src="'customer_web_registration/static/description/m_1.png'"/>
									<span style="text-transform: uppercase;" t-field="user_id.phone"/>
								</div>
							</div>
							<div class="row">
								<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char">
									<img class="saturate" style="max-height: 100%;max-width:9%;object-fit: cover;" t-att-src="'customer_web_registration/static/description/m_2.png'"/>
									<span style="text-transform: uppercase;" t-field="user_id.email"/>
								</div>
							</div>
							<div class="row">
								<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char">
									<img class="saturate" style= "max-height: 100%;max-width:9%;object-fit: cover;" t-att-src="'customer_web_registration/static/description/m_3.png'"/>
									<span style="width: 325px;line-height: 25px;display: inline-table;-webkit-line-clamp: 2;-webkit-box-orient: vertical;padding-left: 1px;padding-right: 17px;text-transform: uppercase;" t-field="user_id.street"/>
									<span style="display: flex; padding-left: 74px; text-transform: uppercase;" t-field="user_id.city"/>
									<span style="display: flex; padding-left: 74px; text-transform: uppercase;" t-field="user_id.state_id.name"/>
									<span style="display: flex; padding-left: 74px; text-transform: uppercase;" t-field="user_id.country_id.name"/>
								</div>
							</div>
							<div class="row">
								<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char">
									<img class="saturate" style="max-height: 100%;max-width:9%;object-fit: cover;" t-att-src="'customer_web_registration/static/description/m_4.png'"/>
									<span style="text-transform: uppercase;" t-field="user_id.website"/>
								</div>
							</div>
							<div class="row">
								<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level" data-name="Field" data-type="char"/>
							</div>
							<div class="row">
								<div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required col-lg-12 o_colored_level">
									<strong>
										<span>
											Follow us
										</span>
									</strong>
								</div>
							</div>
							<div>
								<a style="color:beige;" t-att-href="res_company.social_facebook">
									<img class="saturate_col" style="max-height: 100%;max-width:10%%;object-fit: cover;" t-att-src="'customer_web_registration/static/description/fb.png'"/>
								</a>
								<a style="color:beige;" t-att-href="res_company.social_twitter">
									<img class="saturate_col" style="max-height: 100%;max-width:10%;object-fit: cover;" t-att-src="'customer_web_registration/static/description/twitter.png'"/>
								</a>
								<a style="color:beige;" t-att-href="res_company.social_instagram">
									<img class="saturate_col" style="max-height: 100%;max-width:10%;object-fit: cover;" t-att-src="'customer_web_registration/static/description/insta.png'"/>
								</a>
							</div>
						</div>
					</div>
				</div>
			</div>
		</xpath>
	</template>
</odoo>
